{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "blockspring/copyright-year",
  "version": "1.0.0",
  "title": "Copyright Year",
  "category": "text",
  "keywords": [
    "copyright",
    "year",
    "footer"
  ],
  "icon": "calendar",
  "description": "Dynamically output the current calendar year alongside editable text.",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "p",
      "default": "My Site Name. All Rights Reserved."
    },
    "alignment": {
      "type": "string",
      "default": "none"
    },
    "textColor": {
      "type": "string"
    },
    "customTextColor": {
      "type": "string"
    }
  },
  "example": {
    "content": "Â©YEAR My Site Name. All Rights Reserved."
  },
  "supports": {
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalTextDecoration": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    },
    "__experimentalSelector": "p",
    "__unstablePasteTextInline": true
  },
  "textdomain": "copyright-year",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}